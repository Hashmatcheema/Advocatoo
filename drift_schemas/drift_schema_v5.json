  "fixed_sql": [
    {
      "name": "courts",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"courts\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"name\" TEXT NOT NULL, \"hierarchy\" TEXT NOT NULL, \"city\" TEXT NULL);"
        }
      ]
    },
    {
      "name": "cases",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"cases\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"title\" TEXT NOT NULL, \"case_type\" TEXT NULL, \"status\" TEXT NOT NULL DEFAULT 'Active', \"registration_number\" TEXT NULL, \"date_filed\" TEXT NULL, \"court_id\" INTEGER NULL REFERENCES courts (id), \"bench\" TEXT NULL, \"courtroom_number\" TEXT NULL, \"client_name\" TEXT NULL, \"client_phone\" TEXT NULL, \"client_cnic\" TEXT NULL, \"client_address\" TEXT NULL, \"opposite_party\" TEXT NULL, \"opposite_counsel\" TEXT NULL, \"notes\" TEXT NULL, \"created_at\" TEXT NOT NULL, \"updated_at\" TEXT NOT NULL);"
        }
      ]
    },
    {
      "name": "hearings",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"hearings\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"case_id\" INTEGER NOT NULL REFERENCES cases (id) ON DELETE CASCADE, \"hearing_date\" TEXT NOT NULL, \"hearing_time\" TEXT NULL, \"purpose\" TEXT NULL, \"outcome\" TEXT NULL, \"notes\" TEXT NULL);"
        }
      ]
    },
    {
      "name": "document_folders",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"document_folders\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"case_id\" INTEGER NOT NULL REFERENCES cases (id) ON DELETE CASCADE, \"name\" TEXT NOT NULL, \"created_at\" TEXT NOT NULL);"
        }
      ]
    },
    {
      "name": "document_images",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"document_images\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"folder_id\" INTEGER NOT NULL REFERENCES document_folders (id) ON DELETE CASCADE, \"file_path\" TEXT NOT NULL, \"sort_order\" INTEGER NOT NULL DEFAULT 0, \"added_at\" TEXT NOT NULL);"
        }
      ]
    },
    {
      "name": "activity_log",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"activity_log\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"type\" TEXT NOT NULL, \"case_id\" INTEGER NULL REFERENCES cases (id), \"hearing_id\" INTEGER NULL, \"title\" TEXT NOT NULL, \"created_at\" TEXT NOT NULL);"
        }
      ]
    },
    {
      "name": "notification_history",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"notification_history\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"triggered_at\" TEXT NOT NULL, \"summary\" TEXT NOT NULL);"
        }
      ]
    }
